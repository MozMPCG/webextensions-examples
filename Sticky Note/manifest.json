{
	"manifest_version": 2,
	 "name": "Your Sticky Notes",
	  "version": "1.0",
	  
	"description": "Simple sticky notes utility which provides facility to make small notes of any color and every thing.",
  "homepage_url": "https://github.com/MozMPCG/webextensions-examples/",
  "icons": {
    "128": "icons/logo_128.png",
    "64": "icons/logo_64.png",
    "48": "icons/logo_48.png",
    "32": "icons/logo_32.png"
  },
  "browser_action": {
    "default_icon": "icons/logo_32.png",
    "default_title": "Your Sticky Notes",
    "default_popup": "popup/index.html"
  },
  "permissions": ["storage"]
}
