{

  "manifest_version": 2,
  "name": "Search on the go ",
  "version": "1.0",

  "description": " Right click to search the selected text or IP ",

 "icons": {
    "48": "icons/go48.png"
  },

  "browser_action": {
    "default_icon": "icons/go48.png",
    "default_title": "searchonthego",
    "default_popup": "search.html"
  },

 "permissions": [
        "contextMenus"
],
  "background": {
    "scripts" : ["search-on-the-go.js"]
  }
}
